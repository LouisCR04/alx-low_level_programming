#include <stdio.h>

/**
 *main - Entry point
 *Fibonacci sequence
 *
 *Return:Always 0(Success)
 */

int main(void)
{
	int i, n, m, sum;

	n = 1;
	m = 2;

	printf("1. 2, ");
	for (i = 1; i <= 20; i++)
	{
		sum = n + m;

		printf("%d, ", sum);
	
		if ((i%2) == 0)
		{
			m = sum;
		}
		else if ((i%1) == 0)
			n = sum;
//		printf("...n is %d...m is %d...\n", n, m);
	}
	printf("\n");
	return (0);
}
